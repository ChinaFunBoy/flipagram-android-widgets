<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="flipagram.android.app.kitchensink" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".KitchenSinkActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PercentLayoutActivity"
            android:label="@string/title_activity_percent_layout"
            android:parentActivityName=".KitchenSinkActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="flipagram.android.app.kitchensink.KitchenSinkActivity" />
        </activity>
    </application>

</manifest>
